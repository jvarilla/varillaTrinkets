<!DOCTYPE html>
<html>
  <head>
    <title>VarillaTrinkets</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body bgcolor='LightGray'>
    <table bgcolor='White' width="100%" align"center" style="float:center;">
      <tr><td> <%= render "layouts/header" %> </td></tr>
      <tr><td height='300'align="top"> <div class="pageContent" style="overflow: auto;" > <%= yield %> </div> </td></tr>
      <tr><td> <%= render "layouts/footer" %> </td></tr>
  </body>
</html>
