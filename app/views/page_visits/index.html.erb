<p id="notice"><%= notice %></p>

<h1>Page Visits</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Page</th>
      <th>Visitdate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @page_visits.each do |page_visit| %>
      <tr>
        <td><%= page_visit.user_id %></td>
        <td><%= page_visit.page_id %></td>
        <td><%= page_visit.visitdate %></td>
        <td><%= link_to 'Show', page_visit %></td>
        <td><%= link_to 'Edit', edit_page_visit_path(page_visit) %></td>
        <td><%= link_to 'Destroy', page_visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page Visit', new_page_visit_path %>
