<p id="notice"><%= notice %></p>

<h1>Shipping Options</h1>

<table>
  <thead>
    <tr>
      <th>Optionname</th>
      <th>Shipclass</th>
      <th>Minshiptime</th>
      <th>Minshiptime</th>
      <th>Maxshiptime</th>
      <th>Shipprovider</th>
      <th>Shipcost</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shipping_options.each do |shipping_option| %>
      <tr>
        <td><%= shipping_option.optionname %></td>
        <td><%= shipping_option.shipclass %></td>
        <td><%= shipping_option.minshiptime %></td>
        <td><%= shipping_option.minshiptime %></td>
        <td><%= shipping_option.maxshiptime %></td>
        <td><%= shipping_option.shipprovider %></td>
        <td><%= shipping_option.shipcost %></td>
        <td><%= link_to 'Show', shipping_option %></td>
        <td><%= link_to 'Edit', edit_shipping_option_path(shipping_option) %></td>
        <td><%= link_to 'Destroy', shipping_option, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shipping Option', new_shipping_option_path %>
